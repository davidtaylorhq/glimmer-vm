{
  "name": "@glimmer/vm",
  "version": "0.84.3",
  "type": "module",
  "repository": "https://github.com/glimmerjs/glimmer-vm/tree/master/packages/@glimmer/vm",
  "dependencies": {
    "@glimmer/owner": "workspace:^"
  },
  "devDependencies": {
    "@glimmer-workspace/build-support": "workspace:^",
    "@glimmer/util": "workspace:^",
    "@glimmer/interfaces": "workspace:^",
    "@glimmer/runtime": "workspace:^",
    "@glimmer/node": "workspace:^",
    "@glimmer/global-context": "workspace:^",
    "@glimmer/reference": "workspace:^",
    "@glimmer/destroyable": "workspace:^",
    "@glimmer/program": "workspace:^",
    "@glimmer/validator": "workspace:^",
    "@glimmer/manager": "workspace:^",
    "@glimmer/opcode-compiler": "workspace:^"
  },
  "types": "index.ts",
  "scripts": {
    "test:lint": "eslint .",
    "test:types": "tsc --noEmit -p ../tsconfig.json",
    "build": "rollup -c rollup.config.mjs"
  },
  "workspace": {
    "entry": true,
    "external": {
      "is": [
        "@simple-dom/*"
      ]
    },
    "inline": {
      "is": [
        "@glimmer/*"
      ]
    }
  },
  "main": "index.ts",
  "exports": {
    "types": "./index.ts",
    "development": "./index.ts",
    "default": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  }
}
